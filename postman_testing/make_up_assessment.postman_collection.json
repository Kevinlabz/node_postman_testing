		{
			"name": "Get User By Favorite",
			"event": [
				{
					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"const users = JSON.parse(postman.getGlobalVariable('users'));",
							"let res = JSON.parse(responseBody)",
							"let reactUsers = [];",
							"for (let i = 0; i < users.length; i++) {",
							"    if(users[i].favorites.indexOf('react') !== -1) {",
							"      reactUsers.push(users[i])",
							"    }",
							"}",
							"let flag = true;",
							"for (let i = 0; i < reactUsers.length; i++) {",
							"    if (reactUsers[i].id !== res[i].id) {",
							"        flag = false;",
							"    }",
							"}",
							"tests[\"Status code is 200\"] = responseCode.code === 200;",
							"tests['Correct user objects returned'] = res.length === reactUsers.length && flag;"
						]
					}
				}
			],
